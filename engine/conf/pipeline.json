{
  "data":
  {
    "name": "ReadDataFile",
    "params": {
      "path": "data/kdd_09_data_test.csv",
      "fileType":"csv"
    }
  }
,
  "pipelines": {
    "stages": [
      {
        "estimators": [
          {
            "name": "StandardScaler",
            "params": {
            }
          }
        ]
      },
      {
        "estimators": [
          {
            "name": "IsolationForest",
            "params": {
              "outputFeatureName": "iforest_result"
            }
          },
          {
            "name": "KNNBasedDetection",
            "params": {
              "kList": [3,5],
              "outputFeatureName": "kdist_result",
              "useSubspace": false,
              "subspaceParams": {
                "subspaceMinDim": 10,
                "subspaceMaxDim": 15,
                "subspaceNumSpaces": 2,
                "useFullSpace": false
              }
            }
          },
          {
            "name": "LOF",
            "params": {
              "kList": [3,5],
              "outputFeatureName": "lof_result",
              "useSubspace": false,
              "subspaceParams": {
                "subspaceMinDim": 10,
                "subspaceMaxDim": 15,
                "subspaceNumSpaces": 2,
                "useFullSpace": false
              }
            }
          },
          {
            "name": "Mahalanobis",
            "params": {
              "outputFeatureName": "mahalanobis_result",
              "useSubspace": true,
              "subspaceParams": {
                "subspaceMinDim": 5,
                "subspaceMaxDim": 15,
                "subspaceNumSpaces": 1,
                "useFullSpace": false
              }
            }
          }
        ]
      }
    ]
  },
  "runExplanations": true,
  "finalOutputPath": "results/kdd_09_data_results"
}